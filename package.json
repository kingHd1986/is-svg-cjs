{
  "name": "@novin-develop/is-svg-cjs",
  "version": "5.0.1",
  "description": "is-svg compiled in CommonJS.",
  "keywords": [
    "is-svg",
    "CommonJS",
    "NestJS"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild dist/index.d.ts --platform=node --bundle --format=cjs --outfile=dist/index.js",
    "prepublish": "npm run build",
    "prepare": "husky install",
    "release": "commit-and-tag-version -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kingHd1986/is-svg-cjs"
  },
  "author": "Hamid Zare",
  "license": "MIT",
  "dependencies": {
    "is-svg": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "commit-and-tag-version": "^11.0.0",
    "esbuild": "^0.17.3",
    "husky": "^8.0.3"
  },
  "files": [
    "dist/**/*"
  ]
}
